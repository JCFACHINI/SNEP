<?xml version="1.0" encoding="UTF-8"?>
<modelo>
<elements>
    <id>
        <type>text</type>
        <options>
            <label>Code</label>
            <required>true</required>
        </options>
    </id>
    <name>
        <type>text</type>
        <options>
            <label>Name</label>
            <required>true</required>
            <validators>
                <alnum>
                    <validator>Alnum</validator>
                    <options>
                        <allowWhiteSpace>true</allowWhiteSpace>
                    </options>
                </alnum>
            </validators>
        </options>
    </name>
    <group>
        <type>select</type>
        <options>
            <required>true</required>
            <label>Group</label>
        </options>
    </group>
    <address>
        <type>text</type>
        <options>
            <label>Address</label>
        </options>
    </address>
    <city>
        <type>text</type>
        <options>
            <label>City</label>
        </options>
    </city>
    <state>
        <type>text</type>
        <options>
            <label>State</label>
        </options>
    </state>
    <zipcode>
        <type>text</type>
        <options>
            <label>Zip Code</label>
        </options>
    </zipcode>
    <phone>
        <type>text</type>
        <options>
            <label>Phone</label>
        </options>
    </phone>
    <cell>
        <type>text</type>
        <options>
            <label>Cellphone</label>
        </options>
    </cell>
</elements>
</modelo>
